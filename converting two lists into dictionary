test_keys = ["Ram", "shyam", "kishan", "Hari", "sita"]
test_values = [1,2,3,4,7]

print("Original key list:" + str(test_keys))
print("Original key values:" + str(test_values))

# using dict() and zip() to convert list to dictionary
res = dict(zip(test_keys, test_values))

print("The resultant dictionary is:" + str(res))
